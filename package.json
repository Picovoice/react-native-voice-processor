{
  "name": "@picovoice/react-native-voice-processor",
  "version": "1.2.3",
  "description": "A library for real-time voice processing in React Native.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!test",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "build": "bob build",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install && yarn example pods",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "audio",
    "sound",
    "recorder",
    "record"
  ],
  "repository": "https://github.com/Picovoice/react-native-voice-processor",
  "author": "Picovoice <hello@picovoice.ai> (https://picovoice.ai)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Picovoice/react-native-voice-processor/issues"
  },
  "homepage": "https://github.com/Picovoice/react-native-voice-processor#readme",
  "devDependencies": {
    "@react-native/eslint-config": "0.73.2",
    "@react-native/typescript-config": "0.73.1",
    "@types/jest": "^28.1.2",
    "@types/react": "^18.2.6",
    "@types/react-native": "0.70.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.6.3",
    "pod-install": "^0.1.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-native": "0.73.11",
    "react-native-builder-bob": "^0.20.3",
    "typescript": "5.0.4"
  },
  "resolutions": {
    "@types/react": "^18.2.6"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/",
      "<rootDir>/test/"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "test/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
